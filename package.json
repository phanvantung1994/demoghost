{
  "name": "ghost-web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^4.8.0",
    "@apollo/client": "^3.2.5",
    "@styled-system/theme-get": "^5.1.2",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "antd": "^4.23.1",
    "babel-plugin-styled-components": "^1.11.1",
    "graphql": "^15.3.0",
    "prop-types": "^15.7.2",
    "react": "^18.2.0",
    "react-app-polyfill": "^1.0.6",
    "react-dom": "^18.2.0",
    "react-lazyload": "^3.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "react-spring": "^9.6.0",
    "react-spring-3d-carousel": "^1.3.2",
    "sass": "^1.56.1",
    "styled-components": "^5.2.0",
    "styled-system": "^5.1.5",
    "swiper": "6.8.4",
    "uuid": "^9.0.0"
  },
  "scripts": {
    "env:stag": "cp .env.development .env",
    "env:prod": "cp .env.production .env",
    "dev": "react-scripts --openssl-legacy-provider start",
    "start": "yarn global add serve && serve -s build",
    "prod": "yarn eslint-fix && yarn env:prod && react-app-rewired start",
    "build:stag": "yarn env:stag && react-app-rewired build",
    "build:prod": "yarn env:prod && react-app-rewired build",
    "build": "react-scripts --openssl-legacy-provider build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "validate": "yarn eslint-fix",
    "heroku-postbuild": "yarn build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn validate",
      "pre-push": "yarn validate"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "last 1 ie version"
    ]
  },
  "devDependencies": {
    "customize-cra": "^1.0.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "prettier": "2.1.2",
    "react-app-rewired": "^2.1.6"
  },
  "resolutions": {
    "styled-components": "^5"
  }
}
